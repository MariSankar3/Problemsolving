#include <stack> 
#include <iostream> 
using namespace std; 
 
int main() 
{ 
        // stack container declaration 
    stack<int> mystack1; 
    stack<int> mystack2; 
     
    // pushing elements into first stack 
    mystack1.push(1); 
    mystack1.push(2); 
    mystack1.push(3); 
    mystack1.push(4); 
     
 
        // printing the first stack 
    cout<<"mystack1 = "; 
    while (!mystack1.empty()) { 
        cout<<mystack1.top()<<" "; 
        mystack1.pop(); 
    } }
