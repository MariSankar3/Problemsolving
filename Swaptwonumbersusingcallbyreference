#include <stdio.h>
#include<conio.h>
void swap(int*num1, int*num2)
int main()
{
int num1,num2; 
printf("enter two number:"); 
scanf("%d%d", &num1, &num2); 
printf("before swapping in main n"); 
printf("value of num1 = %d/n",num1); 
printf("value of num2 = %d/n/n",num2); 
swap(&num1,&num2); 
}
{
swap(int*num1, int*num2) 
{ 
int temp; 
temp=*num1; 
num1=*num2;
num2=*temp;
} 
}
{
printf("after swapping in main n"); 
printf("value of num1 =%d/n",num1); 
printf("value of num2 =%d/n/n",num2); 
return 0;
}

