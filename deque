/******************************************************************************

Deque - Double-ended queues are sequence containers
        we can acess element in front and back by push_front or push_back
*******************************************************************************/
#include<bits/stdc++.h>
using namespace std;
int main()
{
    deque<int> dq;
    dq.push_back(10);
    dq.push_front(20);
    dq.push_back(30);
    dq.push_front(15);//
    cout<<dq.back();
    
     dq.pop_back();
     dq.pop_front();
   // for(int a : dq)
  //  cout<<a;

}
